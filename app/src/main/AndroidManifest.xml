<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fs.ps.put.finansominator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.PersonalBudgetActivity"
            android:label="Personal Budgets" />
        <activity
            android:name=".activities.GroupBudgetActivity"
            android:label="Group Budgets" />
        <activity
            android:name=".activities.StatsActivity"
            android:label="Statistics" />
        <activity
            android:name=".activities.ReportsActivity"
            android:label="Reports" />
        <activity
            android:name=".activities.TransactionsActivity"
            android:label="Transactions" />
        <activity android:name=".activities.BudgetViewActivity"></activity>
    </application>

</manifest>